{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/stylesheets/(.*)",
            "dest": "/public/stylesheets/$1"
        },
        {
            "src": "/javascripts/(.*)",
            "dest": "/public/javascripts/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": ["hkg1"],
    "public": true
} 